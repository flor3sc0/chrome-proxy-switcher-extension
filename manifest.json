{
    "manifest_version": 3,
    "name": "Proxy switch with whitelist",
    "version": "1.0",
    "description": "An extension to set a proxy server, with whitelist.",
    "permissions": [
      "proxy",
      "tabs",
      "storage"
    ],
    "background": {
      "service_worker": "src/background.js"
    },
    "action": {
      "default_popup": "src/popup.html",
      "default_icon": {
        "16": "src/icons/icon16.png",
        "48": "src/icons/icon48.png",
        "128": "src/icons/icon128.png"
      }
    },
    "icons": {
      "16": "src/icons/icon16.png",
      "48": "src/icons/icon48.png",
      "128": "src/icons/icon128.png"
    }
  }
  